{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Streamlit: test_web.py",
      "type": "python",
      "request": "launch",
      "module": "streamlit",
      "args": [
        "run",
        "${workspaceFolder}/test_web.py",
        "--server.port",
        "8501",
        "--server.headless",
        "true"
      ],
      "justMyCode": false,
      "console": "integratedTerminal",
      "env": {
        "STREAMLIT_SKIP_EMAIL_PROMPT": "1"
      },
      "cwd": "${workspaceFolder}"
    }
  ]
}
